*GPDFileVersion: "1.0"
*GPDSpecVersion: "1.0"
*GPDFileName:    "AmazonPrinterFilter.gpd"
*Include:        "msxpsinc.gpd"
*Include:        "StdNames.gpd"
*ModelName:      "AWS Virtual Remote Printer Driver"
*MasterUnits:    PAIR(1200, 1200)
*ResourceDLL:    "unires.dll"
*PrinterType:    PAGE
*MaxCopies:      1

*%******************************************************************************
